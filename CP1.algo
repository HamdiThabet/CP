ALGORITHM LDS
VAR
 si: INTEGER :=0;
 set1:={};
 set2:={};
 found :=false;
BEGIN
 for(let i=0 ; i<set1.length ; i++)
{
    found:=false;
    for(let j=0 ; j<set2.length ; j++)
    {
        if(set1[i]==set2[j])
        {found:=true;}

    } END for
    if (found==true)
    {
        si:=si+2*set1[i];
    }
}

END